@model DashBoard

<div class="row">
    <div class="row justify-content-center">
    <div class="col-3  me-4">
        <div class="dashboard-title">
            <div class="metric">
                <p class="number">@Model.TotalBranch</p>
                <p class="label">Number of Branch</p>
            </div>
        </div>
    </div>
    <div class="col-3  me-4">
        <div class="dashboard-title">
            <div class="metric">
                <p class="number">@Model.TotalStudent</p>
                <p class="label">Number of Student</p>
            </div>
        </div>
    </div>
    <div class="col-3">
        <div class="dashboard-title">
            <div class="metric">
                <p class="number">@Model.TotalTeacher</p>
                <p class="label">Number of Teacher</p>
            </div>
        </div>
    </div>
    </div>
</div>

